<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="circular" format="float"/>

    <declare-styleable name="GhostButton">
        <attr name="circular"/>
    </declare-styleable>

    <declare-styleable name="GhostSwitch">
        <attr name="circular"/>
        <attr name="isSelect" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="AutoLayout">
        <attr name="grivate" format="enum"><!--对齐方式 !-->
            <enum name="right" value="0"/>
            <enum name="left" value="1"/>
            <enum name="center" value="2"/>
        </attr>
        <attr name="horizontal_Space" format="dimension"/>
        <attr name="vertical_Space" format="dimension"/>
        <attr name="isFull" format="boolean"/>
    </declare-styleable>


</resources>